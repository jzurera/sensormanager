<%= semantic_form_for @concentrador do |f| %>
  <%= f.inputs do %>
    
	<%= f.input :categoria_equipo, :as => :select, :collection => Hash[CategoriaEquipo.all.map{|d| [d.nombre, d.id]}]  %> 
    <%= f.input :nombre %>
    <%= f.input :descripcion %>
    <%= f.input :estado  %>
    <%= f.input :gis_latitud %>
    <%= f.input :gis_longitud %>
    <%= f.input :mac_addres %>
    <%= f.input :numero_serie %>
    <%= f.input :ip_address %>
    <%= f.input :ip_port %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
